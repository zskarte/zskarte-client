{
  "name": "zskarte-v3",
  "version": "3.0.0-alpha.0",
  "main": "dist/electron/index.js",
  "engines": {
    "node": ">=20",
    "yarn": ">=3"
  },
  "scripts": {
    "ng:serve": "ng serve --port 4300",
    "ng:serve:dev": "ng serve --port 4300 --configuration development",
    "ng:serve:e2e": "npm-run-all -p ng:serve:dev wait",
    "ng:serve:prod": "ng serve --port 4300 --configuration production",
    "ng:build:prod": "ng build --configuration production",
    "ng:build:dev": "ng build --configuration development",
    "electron:serve": "yarn electron:build && wait-on http-get://localhost:4300/ && electron ./ --serve",
    "electron:build": "tsc -p tsconfig.electron.json && electron-builder install-app-deps",
    "build": "run-p ng:build:prod electron:build",
    "start": "run-p ng:serve:dev electron:serve",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "prettify": "prettier --write \"**/*.{js,ts,html,css,json,md}\"",
    "test:e2e": "playwright test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.9",
    "@angular/cdk": "17.0.5",
    "@angular/common": "^17.0.9",
    "@angular/compiler": "^17.0.9",
    "@angular/core": "^17.0.9",
    "@angular/forms": "^17.0.9",
    "@angular/material": "17.0.5",
    "@angular/platform-browser": "^17.0.9",
    "@angular/platform-browser-dynamic": "^17.0.9",
    "@angular/router": "^17.0.9",
    "@types/file-saver": "^2.0.5",
    "@types/geojson": "^7946.0.10",
    "dexie": "^3.2.2",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "immer": "^9.0.18",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "material-icons": "^1.13.1",
    "ngx-indexed-db": "^12.0.0",
    "ol": "^7.2.2",
    "ol-ext": "^4.0.4",
    "proj4": "^2.8.1",
    "rxjs": "~7.8.0",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "ts-md5": "^1.3.1",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular/cli": "~17.0.10",
    "@angular/compiler-cli": "^17.0.9",
    "@playwright/test": "^1.29.2",
    "@types/jasmine": "~4.3.1",
    "@types/jwt-decode": "^3.1.0",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext",
    "@types/proj4": "^2.5.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "electron": "^22.0.2",
    "electron-builder": "^23.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
